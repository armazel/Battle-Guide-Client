{
  "version": "3.1.6",
  "compilerOptions": {
      "jsx": "react",
      "declaration": true,
      "lib": ["es6"],
      "target": "es6",
      "module": "commonjs",
      "noFallthroughCasesInSwitch": true,
      "noImplicitThis": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "outDir": "build/",
      "sourceMap": true,
      "strict": true,
      "allowSyntheticDefaultImports": true,
      "moduleResolution": "node",
      
  },
  "exclude": [
      "build",
      "node_modules",
  ]
}

